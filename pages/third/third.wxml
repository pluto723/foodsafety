<view class="base">
  <view class="title">
    <view id="text1">Welcome!</view>
    <view id="text2">这里是食品配料健康评估</view>
  </view>
  <view class="container">
    <view class="list">
      <block wx:for="{{list}}" wx:key="index">
        <view>{{item}}</view>
      </block>
    </view>
    <image src="{{svgImg}}" id="person" mode="aspectFit"/>
  </view>
  <view class="information_box">
    <view class="information_title">健康评估</view>
    <text class="information_text">{{assess}}</text>
  </view>
  <bar></bar>
</view>